    options: {
        iconSize:     [60,60],
        iconAnchor:   [30,30],
        popupAnchor:  [0,-20],
    }
});

var toiletIcon = new LeafIcon({iconUrl: 'toiletIcon.png'});

<% for ( var i=0; i < data.length; i++ ) {%>
   L.marker( [<%=data[i].geometry.y%>, <%=data[i].geometry.x%>], {icon: toiletIcon}).addTo(mymap).bindPopup( '<a href="/detail/<%=i%>"><%=data[i].attributes.TYPE%><br><%=data[i].attributes.OMSCHRIJVING%></a>');
<% }%>


</script>

