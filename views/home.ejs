<% include header %>
<button class="overviewtitle" data-toggle="collapse" data-target="#content" style="font-size:24px"><i class="fa fa-bars"></i></button>
  
<div class="collapse" id="content">
  <%
  var blogLength = data.length;
  for(var postIndex=0; postIndex<blogLength; postIndex++) {
  %>
  <section>
    <% var post = data[postIndex] %>
    <% include blogpost %>
  </section>
  <%
  }
  %>
</div>
<!--div voor leaflet-map-->
